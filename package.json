{
  "name": "inferno-parcel",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "parcel --no-autoinstall src/index.html",
    "build": "parcel build --no-autoinstall src/index.html",
    "lint": "standard --fix"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-plugin-transform-class-properties": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "babel-preset-react": "latest",
    "parcel-bundler": "latest",
    "standard": "latest"
  },
  "dependencies": {
    "inferno": "^6.1.5",
    "inferno-compat": "^6.1.5"
  },
  "alias": {
    "react": "inferno-compat",
    "react-dom": "inferno-compat",
    "recompose": "incompose"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "browserlist": [
    "last 2 versions",
    ">1%"
  ]
}
